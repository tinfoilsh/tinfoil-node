{
  "name": "tinfoil",
  "version": "0.10.11",
  "description": "Tinfoil secure OpenAI client wrapper",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": {
    "./dist/index.js": "./dist/index.browser.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./ai-sdk-provider": {
      "types": "./dist/ai-sdk-provider.d.ts",
      "default": "./dist/ai-sdk-provider.js"
    },
    "./verifier": {
      "types": "./dist/verifier.d.ts",
      "browser": "./dist/verifier.browser.js",
      "default": "./dist/verifier.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./secure-client": {
      "types": "./dist/secure-client.d.ts",
      "default": "./dist/secure-client.js"
    },
    "./secure-fetch": {
      "types": "./dist/secure-fetch.d.ts",
      "browser": "./dist/secure-fetch.browser.js",
      "default": "./dist/secure-fetch.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:integration": "RUN_TINFOIL_INTEGRATION=true npm test",
    "test:browser:integration": "vitest run --config vite.config.browser-test.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinfoilsh/tinfoil-node.git"
  },
  "keywords": [
    "openai",
    "security",
    "AI",
    "tinfoil"
  ],
  "author": "Tinfoil",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@tinfoil/verifier-browser": "*",
    "@tinfoil/verifier-node": "*",
    "@ai-sdk/openai-compatible": "^1.0.10",
    "ai": "^5.0.19",
    "dotenv": "^17.2.1",
    "ehbp": "^0.0.6",
    "openai": "^5.13.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/browser": "^3.2.4",
    "eslint": "^8.57.0",
    "jsdom": "^25.0.0",
    "playwright": "^1.49.1",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
