{
  "name": "@tinfoilsh/verifier-js",
  "version": "0.1.0",
  "description": "Browser-compatible AMD SEV-SNP attestation verifier",
  "type": "module",
  "main": "dist/index.js",
  "browser": "dist/verifier.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/verifier.min.js",
