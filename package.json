{
  "name": "tinfoil",
  "version": "0.4.4",
  "description": "Tinfoil secure OpenAI client wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp src/wasm-exec.js dist/",
    "test": "jest --config jest.config.cjs",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix --ignore-pattern '**/*.test.ts'",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinfoilsh/tinfoil-node.git"
  },
  "keywords": [
    "openai",
    "security",
    "AI",
    "tinfoil"
  ],
  "author": "Tinfoil",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@ai-sdk/openai-compatible": "^0.2.14",
    "ai": "^4.3.15",
    "dotenv": "^16.4.7",
    "node-fetch": "^2.7.0",
    "openai": "^4.87.3",
    "ts-node": "^10.9.2",
    "undici": "^7.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.2",
    "typescript": "~5.5.0"
  }
}
